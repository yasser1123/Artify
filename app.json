{
  "expo": {
     "name": "Artify",
    "slug": "Artify",
    "version": "1.0.0",
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "sora",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.jsm.sora"
    }
   
  }
}